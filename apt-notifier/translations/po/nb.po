msgid "MX Updater"
msgstr "MX Oppdateringsverktøy"

msgid "MX Updater Help"
msgstr "Hjelpetekst for MX Oppdateringsverktøy"

msgid "MX Updater preferences"
msgstr "Innstillinger for MX Oppdateringsverktøy"

msgid "MX Updater--View and Upgrade, previewing: basic upgrade"
msgstr ""
"MX Oppdateringsverktøy – vis og oppgrader, forhåndsviser enkel oppgradering"

msgid "MX Updater--View and Upgrade, previewing: full upgrade"
msgstr ""
"MX Oppdateringsverktøy – vis og oppgrader, forhåndsviser full oppgradering"

msgid "No logs found."
msgstr "Fant ingen logger."

msgid "No unattended-upgrades dpkg log(s) found."
msgstr "Fant ingen logger for auto-oppgradering med dpkg."

msgid "No updates available"
msgstr "Ingen oppdateringer er tilgjengelige"

msgid "Other options"
msgstr "Andre alternativer"

msgid "Preferences"
msgstr "Innstillinger"

