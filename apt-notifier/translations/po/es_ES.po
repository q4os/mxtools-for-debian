msgid "MX Updater"
msgstr "MX Actualizador"

msgid "MX Updater Help"
msgstr "MX Ayuda del Actualizador"

msgid "MX Updater preferences"
msgstr "MX Preferencias del Actualizador"

msgid "MX Updater--View and Upgrade, previewing: basic upgrade"
msgstr "MX Actualizador--Ver y Actualizar, vista previa: actualización básica"

msgid "MX Updater--View and Upgrade, previewing: full upgrade"
msgstr ""
"MX Actualizador--Ver y Actualizar, vista previa: actualizacion completa"

msgid "No logs found."
msgstr "No se encontraron registros."

msgid "No unattended-upgrades dpkg log(s) found."
msgstr "No se encontraron registros de dpkg de actualizaciones desatendidas."

msgid "No updates available"
msgstr "No hay actualizaciones disponibles"

msgid "Other options"
msgstr "Otras opciones"

msgid "Preferences"
msgstr "Preferencias"

