msgid "{num} new update available"
msgid_plural "{num} new updates available"
msgstr[0] "Há {num} nova atualização disponível"
msgstr[1] "Há {num} novas atualizações disponíveis"
msgstr[2] "Há {num} novas atualizações disponíveis"

#. TRANSLATORS:
