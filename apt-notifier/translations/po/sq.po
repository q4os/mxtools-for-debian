#, python-brace-format
msgid "You have {num} new update available"
msgid_plural "You have {num} new updates available"
msgstr[0] "Keni të gatshëm {num} përditësim të ri"
msgstr[1] "Keni të gatshme {num} përditësime të reja"

msgid "automatically close terminal window when full/basic upgrade complete"
msgstr ""
"mbylle vetvetiu dritaren e terminalit, kur plotësohet përmirësimi i "
"plotë/elementar"

msgid "automatically close terminal window when upgrade complete"
msgstr "mbylle vetvetiu dritaren e terminalit, kur plotësohet përmirësimi"

msgid "basic upgrade"
msgstr "përmirësim elementar"

msgid "basic upgrade complete (or was canceled)"
msgstr "përmirësim elementar i plotësuar (ose u anulua)"

msgid "check for autoremovable packages after full/basic upgrade"
msgstr ""
"kontrollo për paketa të vetëheqshme, pas përmirësimi të plotë/elementar"

msgid "classic"
msgstr "klasike"

msgid "full upgrade"
msgstr "përmirësim i plotë"

msgid "full upgrade   (recommended)"
msgstr "përmirësim i plotë   (e rekomanduar)"

msgid "full upgrade complete (or was canceled)"
msgstr "përmirësim tërësor i plotësuar (ose u anulua)"

msgid "opens MX Updater 'View and Upgrade' window"
msgstr "hap dritaren “Shiheni dhe Përmirësojeni” të Përditësuesit MX"

msgid "opens Synaptic"
msgstr "hap Synaptic-un"

msgid "press any key to close"
msgstr "shtypni çfarëdo tasti që të mbyllet"

msgid "press any key to stop automatic closing"
msgstr "shtypni çfarëdo tasti, për të ndalur mbylljen automatike"

msgid "pulse"
msgstr "puls"

msgid "start MX Updater at login"
msgstr "nise Përditësuesin MX gjatë hyrjes"

msgid "switch to basic upgrade"
msgstr "kalo te përmirësim elementar"

msgid "switch to full upgrade"
msgstr "kalo te përmirësim i plotë"

msgid "this terminal window can now be closed"
msgstr "kjo dritare terminali tani mund të mbyllet"

msgid "update automatically   (will not add new or remove existing packages)"
msgstr ""
"përditësoje automatikisht   (s’do të shtohen paketa të reja apo hiqen "
"ekzistuese)"

msgid "upgrade"
msgstr "përmirësim"

msgid "use desktop notifications"
msgstr "përdor njoftime desktopi"

msgid "use transparent interior for no-updates wireframe"
msgstr ""

