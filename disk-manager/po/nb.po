#: ../DiskManager/Fstab/FstabHandler.py:435
msgid "Unmounting disabled devices?"
msgstr "Avmonter avslåtte enheter?"

#: ../DiskManager/Fstab/FstabHandler.py:436
msgid "You disabled the following devices:"
msgstr "Du slo av følgende enheter:"

#: ../DiskManager/Fstab/FstabHandler.py:436
msgid "Do you want to unmount them?"
msgstr "Vil du avmontere dem?"

#: ../DiskManager/Fstab/FstabHandler.py:438
msgid "Unmount selected"
msgstr "Avmonter valgt"

#: ../DiskManager/Fstab/FstabHandler.py:447
msgid "Reboot required"
msgstr "Omstart nødvendig"

#: ../DiskManager/Fstab/FstabHandler.py:448
msgid ""
"In order to apply all your changes,\n"
"you'll need to reboot your computer."
msgstr ""
"Datamaskinen må startes på nytt\n"
"for å ta i bruk alle endringene."

#: ../DiskManager/Fstab/Mounter.py:56
msgid "Mounting partition"
msgstr "Monterer partisjon"

#: ../DiskManager/Fstab/Mounter.py:58
msgid "Unmounting partition"
msgstr "Avmonterer partisjon"

#: ../DiskManager/Fstab/Mounter.py:60
msgid "Applying changes"
msgstr "Tar i bruk endringer"

#: ../DiskManager/Fstab/Mounter.py:66
#, python-format
msgid "Unmounting %s"
msgstr "Avmonterer %s"

#: ../DiskManager/Fstab/Mounter.py:80
#, python-format
msgid "Mounting %s"
msgstr "Monterer %s"

#: ../DiskManager/Fstab/Mounter.py:115
msgid "Unmounting failed"
msgstr "Avmontering mislyktes"

#: ../DiskManager/Fstab/Mounter.py:116
#, python-format
msgid "Unmounting <i>%s</i> failed because of the following error:"
msgstr "Det oppstod en feil ved avmontering av <i>%s</i>:"

#. TRANSLATORS:
#. Keep the English term 'lazy' for the 'lazy' unmount option
#: ../DiskManager/Fstab/Mounter.py:122
msgid "You can also use the 'lazy' options to detach the filesystem now."
msgstr "Alternativene 'lazy' kan velges for å avmontere filsystemet nå."

#: ../DiskManager/Fstab/Mounter.py:127
#, python-format
msgid ""
"Unmounting <i>%s</i> failed, because\n"
"it is currently used by the following applications:"
msgstr ""
"Følgende programmer forhindrer \n"
"avmontering av <i>%s</i>:"

#: ../DiskManager/Fstab/Mounter.py:129
msgid "Close all applications that use it and retry to unmount."
msgstr ""
"Lukk alle programmer som bruker filsystemet og forsøk avmontering på nytt."

#: ../DiskManager/Fstab/Mounter.py:132
msgid "The filesystem might be temporarily busy. Wait few seconds and retry."
msgstr ""
"Filsystemet er midlertidig opptatt. Vent noen sekunder og forsøk igjen."

#. TRANSLATORS:
#. Keep the English term 'lazy' for the 'lazy' unmount option
#: ../DiskManager/Fstab/Mounter.py:137
msgid "Unmount with the 'lazy' option"
msgstr "Avmonter med alternativet 'lazy'"

#: ../DiskManager/Fstab/Mounter.py:138
msgid "Retry unmount"
msgstr "Forsøk avmontering på nytt"

#. TRANSLATORS:
#. Keep the English term 'lazy' for the 'lazy' unmount option
#: ../DiskManager/Fstab/Mounter.py:143
msgid "Even with the lazy option, unmounting failed."
msgstr "Avmontering mislyktes, selv ved bruk av alternativet 'lazy'."

#: ../DiskManager/Fstab/Mounter.py:155
msgid "Mounting failed"
msgstr "Montering mislyktes"

#: ../DiskManager/Fstab/Mounter.py:156
#, python-format
msgid "Mounting <i>%s</i> failed because of the following error:"
msgstr "Det oppstod en feil ved montering av <i>%s</i>:"

#: ../DiskManager/Fstab/Mounter.py:159
msgid "Retry mount"
msgstr "Forsøk montering på nytt"

#: ../DiskManager/Fstab/Mounter.py:190
msgid "You can try one of the following actions:"
msgstr "Forsøk én av de følgende handlingene:"

#: ../DiskManager/Fstab/Mounter.py:193
#, python-format
msgid "It seems that option '%s' is not allowed."
msgstr "Alternativet '%s' kan ikke velges."

#: ../DiskManager/Fstab/Mounter.py:195
msgid "Return options to defaults"
msgstr "Tilbakestill alternativer til standardverdier"

#: ../DiskManager/Fstab/Mounter.py:197
#, python-format
msgid "Don't use the '%s' option"
msgstr "Ikke bruk alternativet '%s'"

#: ../DiskManager/Fstab/Mounter.py:199
#, python-format
msgid "It seems that fs type '%s' is not valid."
msgstr "FS-typen «%s» er ugyldig."

#: ../DiskManager/Fstab/Mounter.py:203
#, python-format
msgid "Using driver '%s' (%s)"
msgstr "Bruker driver '%s' (%s)"

#. TRANSLATORS:
#. Keep the English term 'force' for the 'force' unmount option
#: ../DiskManager/Fstab/Mounter.py:211
msgid ""
"You can try to use the 'force' option. Be aware that this could be risky."
msgstr ""
"Alternativet 'force' er en mulighet, men legg merke til at det er "
"risikabelt."

#: ../DiskManager/Fstab/Mounter.py:212
msgid "Use the 'force' option"
msgstr "Bruk alternativet 'force'"
